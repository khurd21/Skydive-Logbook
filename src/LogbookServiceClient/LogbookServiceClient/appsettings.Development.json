{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    }
  },
  "DynamoDb": {
    "LocalMode": true,
    "ServiceUrl": "http://localhost:8000",
    "ConsistentRead": true,
    "IgnoreNullValues": true,
    "SkipVersionCheck": true,
    "TableNamePrefix": null
  },
  "Jwt": {
    "ExpiresInDays": 1
  }
}
